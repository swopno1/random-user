{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "functions": {
    "index.js": {
      "runtime": "nodejs16.x",
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
